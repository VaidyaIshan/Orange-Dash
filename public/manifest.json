{
  "manifest_version": 3,
  "name": "orangedash",
  "version": "1.0.0",
  "description": "A Chrome extension for quick access to favorite web applications.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "android-chrome-512x512.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "48": "android-chrome-512x512.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}